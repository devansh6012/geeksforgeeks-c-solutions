#include <iostream>
using namespace std;
int main() 
{
	int t;
	cin>>t;
	for(int g=0;g<t;g++)
	{
	    int a,b;
	    cin>>a>>b;
	    int arr[a];
	    for(int i=0;i<a;i++)
	    {
	        cin>>arr[i];
	    }
	    int k=arr[b-1];
	    arr[b-1]=arr[a-b];
	    arr[a-b]=k;
	    for(int i=0;i<a;i++)
	    {
	        cout<<arr[i]<<" ";
	    }
	    cout<<endl;
	}
	return 0;
}
